
			<!-- BEGIN PAGE HEADER-->
			<div class="row-fluid">
				<div class="span12">
					
					<!-- BEGIN PAGE TITLE & BREADCRUMB-->		
					<h3 class="page-title">Dashboard</h3>
					<%= render 'accounts/breadcrumb' %>
					

				</div><!-- /.span12 -->
			</div><!-- /.row-fluid -->

			<div id="page" class="dashboard">

				<div class="row-fluid">

					<div class="span4">
						<div class="widget">
							<div class="widget-title">
								<h4>Your Information</h4>
							</div><!-- /.widget-title -->
							<div class="widget-body">
								<table class="table table-striped">
									<tbody>
										<tr>
											<td><strong>Name:</strong></td>
											<td><%= current_user.first_name %> <%= current_user.last_name %></td>
										</tr>
										<tr>
											<td><strong>Email:</strong></td>
											<td><%= current_user.email %></td>
										</tr>
										<tr>
											<td><strong>Plan:</strong></td>
											<td><%= @plan.name %></td>
										</tr>
										<tr>
											<td><strong>Status:</strong></td>
											<td><%= @status %></td>
										</tr>
									</tbody>
								</table>
							</div><!-- /.widget-body -->
						</div><!-- /.widget -->
					</div><!-- /.span4 -->

					<div class="span4">
						<div class="widget">
							<div class="widget-title">
								<h4>Locations</h4>
								<span class="tools">
									<a href="#" class="show-popover" data-toggle="popover" data-placement="top" data-content="No problem, we can help. Contact us by email at info@pickgrapevine.com or call us at 210-549-7799." title="" data-original-title="Need to edit your location?"><i class="icon-pencil"></i></a>
								</span>
							</div><!-- /.widget-title -->
							<div class="widget-body">
								<table class="table table-striped">
									<thead>
										<tr>
											<th>Name</th>
											<th>Address</th>
										</tr>
									</thead>
									<tbody>
										<% @items.each do |item| %>
										<tr>
											<td><%= item.name %></td>
											<td><%= item.street_address %></td>
										</tr>
										<% end %>
									</tbody>
								</table>
										
							</div><!-- /.widget-body -->
						</div><!-- /.widget -->
					</div><!-- /.span4 -->

					<div class="span4">
						<div class="widget">
							<div class="widget-title">
								<h4>Plan Details</h4>
							</div><!-- /.widget-title -->
							<div class="widget-body">
								Some kinda shit
							</div><!-- /.widget-body -->
						</div><!-- /.widget -->
					</div><!-- /.span4 -->



				</div><!-- /.row-fluid -->

			</div><!-- /#page -->
<script>
$('.show-popover').popover();
</script>

